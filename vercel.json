{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://damatatechnology.com.br/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://tgi-projeto-front-end.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ],
  "cookies": [
        {
          "SameSite": "none"
        }
  ]
}
